        "DescribeSpotPriceHistory": {
            "limit_key": "MaxResults",
            "input_token": "NextToken",
            "output_token": "NextToken",
            "result_key": "SpotPriceHistory"
        },
        "DescribeVolumeStatus": {
            "limit_key": "MaxResults",
            "input_token": "NextToken",
            "output_token": "NextToken",
